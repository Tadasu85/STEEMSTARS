<center>
<h1>Account: <%= params[:account_id] %></h1>

<p><%= pluralize(@follows.count, 'follows') %></p>
</center>
<ul>
  <% @follows.each do |follow| %>
    <li><%= link_to follow, account_follows_path(account_id: follow.gsub('.', ',')) %></li>
  <% end %>
</ul>

[
<%= link_to "Previous", :back %>
|
<%= link_to "Next", account_follows_path(account_id: @account_id, after: @follows.last) %>
]